generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Dashboard {
  id              String   @id @default(uuid())
  pais            String
  estado          String
  sexo            String
  faixaEtaria     String
  quantidadeTaxa  Int
  totalTaxa       Decimal  @db.Decimal(10, 2)
  data            DateTime
  criadoEm        DateTime @default(now())
}
